<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

  <!-- Servlet Mapping. -->
    <servlet>
        <servlet-name>ReadBoardStateServlet</servlet-name>
        <servlet-class>com.google.research.bleth.servlets.ReadBoardStateServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ListStrategiesServlet</servlet-name>
        <servlet-class>com.google.research.bleth.servlets.ListStrategiesServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NewSimulationServlet</servlet-name>
        <servlet-class>com.google.research.bleth.servlets.NewSimulationServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ListSimulationsServlet</servlet-name>
        <servlet-class>com.google.research.bleth.servlets.ListSimulationsServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteSimulationServlet</servlet-name>
        <servlet-class>com.google.research.bleth.servlets.DeleteSimulationServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ReadDistanceStatisticsServlet</servlet-name>
        <servlet-class>com.google.research.bleth.servlets.ReadDistanceStatisticsServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ReadObservedStatisticsServlet</servlet-name>
        <servlet-class>com.google.research.bleth.servlets.ReadObservedStatisticsServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EnqueueSimulationServlet</servlet-name>
        <servlet-class>com.google.research.bleth.servlets.EnqueueSimulationServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EnqueueExperimentServlet</servlet-name>
        <servlet-class>com.google.research.bleth.servlets.EnqueueExperimentServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NewExperimentSimulationServlet</servlet-name>
        <servlet-class>com.google.research.bleth.servlets.NewExperimentSimulationServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>ReadBoardStateServlet</servlet-name>
        <url-pattern>/read-board-state</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ListStrategiesServlet</servlet-name>
        <url-pattern>/list-strategies</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NewSimulationServlet</servlet-name>
        <url-pattern>/new-simulation</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ListSimulationsServlet</servlet-name>
        <url-pattern>/list-simulations</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteSimulationServlet</servlet-name>
        <url-pattern>/delete-simulation</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ReadDistanceStatisticsServlet</servlet-name>
        <url-pattern>/read-distance-stats</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ReadObservedStatisticsServlet</servlet-name>
        <url-pattern>/read-observed-stats</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EnqueueSimulationServlet</servlet-name>
        <url-pattern>/enqueue-simulation</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EnqueueExperimentServlet</servlet-name>
        <url-pattern>/enqueue-experiment</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NewExperimentSimulationServlet</servlet-name>
        <url-pattern>/new-experiment-simulation</url-pattern>
    </servlet-mapping>

</web-app>
